{
    "name": "php-panel/ladmin-ext-api-tester",
    "description": "Api tester for laravel",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "api",
        "tester"
    ],
    "homepage": "https://github.com/php-panel/ladmin-ext-api-tester",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "leeqvip",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "php-panel/laravel-admin": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "laravel/laravel": "~12.0"
    },
    "autoload": {
        "psr-4": {
            "Ladmin\\ApiTester\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladmin\\ApiTester\\ApiTesterServiceProvider"
            ]
        }
    }
}